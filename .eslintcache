tarea>\n            <br></br>\n            <button>Update</button>\n          </form>\n        ) : (\n          <>\n            <h2>{this.props.opinion.title}</h2>\n            <p>{this.props.opinion.content}</p>\n            <button onClick={this.onClickHandler}>Disagree</button>\n            <button onClick={this.onClickHandler}>Agree</button>\n          </>\n        )}\n      </div>\n    );\n  }\n}\n\nconst msp = (state) => {\n  return { currentUser: state.currentUser };\n};\n\nconst mdp = (dispatch) => {\n  return {\n    editPost: (opinion) => dispatch(editOpinion(opinion)),\n    deletePost: (opinion) => dispatch(deleteOpinion(opinion))\n   };\n};\n\nexport default withRouter(connect(msp, mdp)(Opinion));\n","/Users/ajakcyer/Flatiron/code/project-mod5/opini8d-frontend/src/ReuseComponents/OpinionCard.js",["82","83","84"],"import React from \"react\";\nimport { NavLink, Route, Switch, withRouter } from \"react-router-dom\";\n\nexport const OpinionCard = ({ opinion }) => {\n  return (\n    <div>\n      {/* <h4>By: {opinion.user.username}</h4> */}\n      <NavLink to={`/opinions/${opinion.id}`}>\n        <h3>{opinion.title}</h3>\n      </NavLink>\n      <hr></hr>\n    </div>\n  );\n};\n","/Users/ajakcyer/Flatiron/code/project-mod5/opini8d-frontend/src/ReuseComponents/CreateOpinion.js",[],"/Users/ajakcyer/Flatiron/code/project-mod5/opini8d-frontend/src/Profile/MyOpinions.js",["85"],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":2,"column":34,"nodeType":"92","messageId":"93","endLine":2,"endColumn":44},{"ruleId":"90","severity":1,"message":"94","line":2,"column":10,"nodeType":"92","messageId":"93","endLine":2,"endColumn":17},{"ruleId":"90","severity":1,"message":"95","line":3,"column":10,"nodeType":"92","messageId":"93","endLine":3,"endColumn":30},{"ruleId":"90","severity":1,"message":"96","line":4,"column":10,"nodeType":"92","messageId":"93","endLine":4,"endColumn":17},{"ruleId":"90","severity":1,"m